generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model addresses {
  id           Int       @id @default(autoincrement())
  street       String?   @db.VarChar(255)
  city         String?   @db.VarChar(255)
  region       String?   @db.VarChar(255)
  country      String?   @db.VarChar(255)
  zip          String?   @db.VarChar(255)
  latitude     Float?    @db.Float
  longitude    Float?    @db.Float
  full_address String?   @db.VarChar(255)
  mod_id       String?   @db.VarChar(255)
  created_at   DateTime? @db.DateTime(0)
  updated_at   DateTime? @db.DateTime(0)
  deleted_at   DateTime? @db.DateTime(0)
  zip_bak      String?   @db.VarChar(255)
}

model admin_statistics {
  id                             Int       @id @default(autoincrement())
  total_raised_this_year         Int?
  launches_this_year             Int?
  sales_this_year                Int?
  users_this_year                Int?
  active_salesreps               Int?
  donations_this_year            Int?
  email_count_today              Int?
  users_today                    Int?
  total_raised_today             Int?
  self_signups_today             Int?
  game_plan_calls_today          Int?
  fundraisers_starting_yesterday Int?
  fundraisers_starting_today     Int?
  fundraisers_starting_tomorrow  Int?
  fundraisers_ending_yesterday   Int?
  fundraisers_ending_today       Int?
  fundraisers_ending_tomorrow    Int?
