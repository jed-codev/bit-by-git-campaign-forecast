  created_at     DateTime? @db.DateTime(0)
  updated_at     DateTime? @db.DateTime(0)

  @@index([am_salesrep_id], map: "index_am_salesrep_pay_groups_on_am_salesrep_id")
  @@index([pay_group_id], map: "index_am_salesrep_pay_groups_on_pay_group_id")
}

model am_salesreps {
  id                  Int       @id @default(autoincrement())
  namely_id           String?   @db.VarChar(255)
