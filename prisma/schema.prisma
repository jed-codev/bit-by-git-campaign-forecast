model api_keys {
  id           Int       @id @default(autoincrement())
  user_id      Int?
  access_token String?   @db.VarChar(255)
  status       String?   @default("active") @db.VarChar(255)
  created_at   DateTime? @db.DateTime(0)
  updated_at   DateTime? @db.DateTime(0)

  @@index([access_token], map: "index_api_keys_on_access_token")
  @@index([user_id], map: "index_api_keys_on_user_id")
}

model ar_internal_metadata {
  key        String   @id @db.VarChar(255)
  value      String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model audit_entries {
  id            Int      @id @default(autoincrement())
  event_type    String?  @db.VarChar(255)
  source_id     Int?
  source_type   String?  @db.VarChar(255)
  target_id     Int?
  target_type   String?  @db.VarChar(255)
  context_id    Int?
  context_type  String?  @db.VarChar(255)
  payload       String?  @db.Text
  request_id    String?  @db.VarChar(255)
  ip_address    String?  @db.VarChar(255)
  user_agent    String?  @db.Text
  result_status String?  @db.VarChar(255)
  created_at    DateTime @db.DateTime(0)
  updated_at    DateTime @db.DateTime(0)

  @@index([context_id, context_type], map: "index_audit_entries_on_context_id_and_context_type")
  @@index([event_type], map: "index_audit_entries_on_event_type")
  @@index([source_id, source_type], map: "index_audit_entries_on_source_id_and_source_type")
  @@index([target_id, target_type], map: "index_audit_entries_on_target_id_and_target_type")
}

