  trainer_id          Int?
  sales_manager_id    Int?
  am_id               Int?
  address             String?   @db.VarChar(255)
  city                String?   @db.VarChar(255)
  region              String?   @db.VarChar(255)
  zip                 String?   @db.VarChar(255)
  country             String?   @db.VarChar(255)
  pay_group_name      String?   @db.VarChar(255)
  remote              Boolean   @default(false)
  start_date          DateTime? @db.Date
  end_date            DateTime? @db.Date
  draw_amount         Int?
  base                Int?
  draw_end_date       DateTime? @db.Date
  commission_end_date DateTime? @db.Date
  phone               String?   @db.VarChar(255)
  pants_size          String?   @db.VarChar(255)
  shirt_size          String?   @db.VarChar(255)
  territory           String?   @db.VarChar(255)
  territory_zip_codes String?   @db.Text
  territory_zone      String?   @db.VarChar(255)
  territory_map       String?   @db.VarChar(255)
