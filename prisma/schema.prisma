  @@index([period_end, pay_subtype], map: "index_commissions_on_period_end_and_pay_subtype")
  @@index([period_end, pay_type], map: "index_commissions_on_period_end_and_pay_type")
  @@index([salesrep_id], map: "index_commissions_on_salesrep_id")
  @@index([salesrep_id, period_end], map: "index_commissions_on_salesrep_id_and_period_end")
  @@index([salesrep_id, period_end, pay_subtype], map: "index_commissions_on_salesrep_id_and_period_end_and_pay_subtype")
  @@index([salesrep_id, period_end, pay_type], map: "index_commissions_on_salesrep_id_and_period_end_and_pay_type")
}

