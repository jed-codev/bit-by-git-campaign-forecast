  salesrep_type       String?   @db.VarChar(255)

  @@index([am_id], map: "index_am_salesreps_on_am_id")
  @@index([namely_id], map: "index_am_salesreps_on_namely_id")
  @@index([salesrep_id], map: "index_am_salesreps_on_salesrep_id")
}

