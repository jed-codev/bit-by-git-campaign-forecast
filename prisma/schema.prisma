  notes                   String?   @db.Text
  registration_cost_cents Int?
  hotels_booked           Boolean?
  door_prize_needed       Boolean?
  internet_provided       Boolean?
  electricity_provided    Boolean?
  clinic_organizer_id     Int?
  podio_item_id           Int?      @unique(map: "index_clinics_on_podio_item_id")
  podio_item_url          String?   @db.VarChar(255)
  start_datetime          DateTime? @db.DateTime(0)
  end_datetime            DateTime? @db.DateTime(0)
  setup_time_start        DateTime? @db.DateTime(0)
  setup_time_end          DateTime? @db.DateTime(0)
  created_at              DateTime? @db.DateTime(0)
  updated_at              DateTime? @db.DateTime(0)

  @@index([clinic_organizer_id], map: "index_clinics_on_clinic_organizer_id")
  @@index([status], map: "index_clinics_on_status")
}

model commissions {
  id                     Int       @id @default(autoincrement())
  fundraiser_id          Int?
  salesrep_id            Int?
  adjusted_id            Int?
  shop_order_gid         String?   @db.VarChar(255)
  amount_cents           Int?
  adjust_cents           Int?
